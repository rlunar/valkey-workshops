# Database Configuration
# Supported: mysql, mariadb, postgresql
DB_ENGINE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=airportdb

# Cache Configuration
# Supported: redis, valkey, memcached
CACHE_ENGINE=redis
CACHE_HOST=localhost
CACHE_PORT=6379

# Cache TTL (Time To Live) in seconds
# Default: 3600 (1 hour)
CACHE_TTL=3600
